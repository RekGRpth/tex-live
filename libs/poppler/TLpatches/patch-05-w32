 struct StatMtim
 {
   static const struct timespec& value(const Stat& stbuf) {
+#if defined(_WIN32)
+    static struct timespec t_spec;
+    t_spec.tv_sec=stbuf.st_mtime;
+    t_spec.tv_nsec=0;
+    return t_spec;
+#else
     return stbuf.st_mtim;
+#endif
   }
 };
 
